{
  "manifest_version": 2,
  "name": "Todo4U Demo",
  "description": "Simple Todo extension for codezilla.",
  "version": "1.0",
  "icons": {
    "48":"icons/clock.jpg"
    },
  "permissions":["storage","contextMenus","notifications","tabs"],
  "background":{
    "scripts":["background.js"]
  },
  "browser_action":{
    "default_icon":"icons/clock.jpg",
    "default_title":"Todo4U",
    "default_popup":"index.html",
    "browser_style":true
  },
  "page_action":{
    "default_icon":"icons/clock.jpg",
    "browser_style":true
  },
  "applications": {
    "gecko": {
      "id": "dilanSachi@github.com"
    }
  }
}
